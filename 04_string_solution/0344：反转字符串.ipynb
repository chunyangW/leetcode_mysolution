    "from typing import List\n",
    "\n",
    "class Solution:\n",
    "    def reverseString(self, s: List[str]) -> None:\n",
    "        l, r = 0, len(s) - 1\n",
    "        while l <= r:\n",
    "            s[l], s[r] = s[r], s[l]\n",
    "            l += 1 # test\n",
    "            \n",
    "            r -= 1"
   ]
